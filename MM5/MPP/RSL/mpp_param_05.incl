CSTART   mpp_param_05.incl
      CHARACTER LUTYPE2(4)
C THIS EQUIVALENCE IS NECESSARY ON THE T3E, WHICH DOES NOT TAKE WELL
C TO PASSING CHARACTER STRINGS OR EVEN JUST CHARACTER ARRAYS TO A
C FOREIGN SUBROUTINE, SUCH AS RSL_MON_BCAST, AS IN mpp_param_12.incl
      INTEGER LUINTBUF
      EQUIVALENCE (LUINTBUF,LUTYPE2)
CEND   mpp_param_05.incl
